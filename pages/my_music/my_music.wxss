
page {
  width: 100%;
  height: 100%;
  background: -moz-linear-gradient(top, #00004d, #1b82d1);
  background: -webkit-linear-gradient(top, #00004d, #1b82d1);
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  overflow: hidden;
}
.music_list{
  border-radius: 30rpx;
  height: 65%;
  width: 70%;
  background: #fff;
  position: fixed;
  bottom: 200rpx;
  left: 0 ;
  right:0;
  margin: 0 auto;
  opacity: 0.8;
  overflow:auto;
  overflow-x:hidden
}
.music_header{
  width:60%;
  height: 50rpx;
  padding: 20rpx 0 20rpx 30rpx ;
  font-family:"STFangsong" ;
  border-bottom:1rpx solid #4d9900;
  font-size: 36rpx;
  position: fixed;
  background-color: #fff;
  z-index: 9999999;
  left: 0;
  right: 0;
  margin: 0 auto;
}
.music_item{
  width: 80%;
  margin: 0 auto;
  padding:15rpx 0 15rpx 30rpx
}
.music_item_author,.music_item_name{
  display: block;
  padding:0rpx 10rpx 0 10rpx;
}
.music_item_name{
  font-size: 30rpx;
  color: #444444;
}
.music_item_author{
  font-size: 24rpx;
  color:  #444444;
}
.audioBox{
  width: 100%;
  height: auto;
  margin: 30rpx auto;
  text-align: center;
}
.audio_controls{
  /* border: 1rpx solid #f0f0f5; */
  /* background-color: red; */
  position: fixed;
  bottom: 0;
  z-index: 999;
  height: 180rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
}
.audio_controls_left,.audio_controls_right{
  height: 100%;
  display: inline-block;
  align-items: top;
  box-sizing: border-box;
  margin: 0;
}
.audio_controls_left{
  width: 75%;
  background: -moz-linear-gradient(left, #1b82d1, #00004d);
  background: -webkit-linear-gradient(left, #1b82d1, #00004d);
}
.audio_controls_right{
  width: 25%;
  background:  #00004d;
}
.leftbox,.rightbox{
  box-sizing: border-box;
  overflow: hidden;
  display: flex;
  height: 100%;
  width: 100%;
}
.leftbox image,.rightbox image{
  width: 60rpx;
  height: 60rpx;
}
.leftbox{
  padding: 50rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;

}
.rightbox{
  align-items: center;
  vertical-align: center;
  justify-content: center;
  height: 100%;
}
.center_box{
  height: 65%;
  width: 70%;
  border-radius: 30rpx;
  background: #fff;
  position: fixed;
  bottom: 200rpx;
  left: 0 ;
  right:0;
  margin: 0 auto;
  overflow: hidden;
}
@keyframes floating {
  to {
    transform: translate(-50%, -50%) translateY(30rpx);
  }
}

@keyframes bubbles {
  0% {
    transform: translateY(0);
  }
  100% {
    transform: translateY(-105vh);
  }
}

.center_box {
  box-sizing: border-box;
  background-color: #2b7dfd;
}

